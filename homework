#7번
cin >> a >> b ; ( " " 삭제 )
cout << "정수부의 합은" << (int)a + (int)b << " 입니다." << endl; 

#8번
#include <iostream>
using namespace std;

int main() {

	double fd, cd;
	cout << "화씨온도입력: ";
	cin >> fd;
	cd = (fd - 32) * 5 / 9;

	cout.precision(5);
	cout << fixed;
	cout << "섭씨온도는" << cd << " 입니다." << endl;

	return 0;
}

#9번
#include <limits> 추가
cout << numeric_limits<long long>::max() << endl;
cout << numeric_limits<long long>::min() << endl; ( <> 안에 long long 추가)

#10번
#include <iostream>
using namespace std;
int main() { 
    int a;
    cout << "정수 입력: ";
    cin >> a;
    cout << showbase;
    cout << oct << a << endl;
    cout << hex << a << endl;
 return 0;
}

